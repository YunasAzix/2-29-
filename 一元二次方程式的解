A=float(input('請輸入第一個數字:'))
B=float(input('請輸入第二個數字:'))
C=float(input('請輸入第三個數字:'))
if (B**2)-(4*A*C)<0:
  print('無解')
else:
  answer1=(B*-1+(((B**2)-(4*A*C))**(1/2)))/(A*2)
  answer2=(B*-1-(((B**2)-(4*A*C))**(1/2)))/(A*2)
  if float(answer1)==float(answer2):
    print("x=",float(answer1))
  else:
    print("x=",float(answer1),"or",float*(answer2))
